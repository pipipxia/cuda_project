{
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python 调试程序: 当前文件",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "preLaunchTask": "允许非root用户附加进程",
            // "preLaunchTask": "构建项目",
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2.0"
            }
        },
        {
            "name": "c++ gdb Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "/home/lihuixiang/.conda/envs/tvm/bin/python",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-prettey-printing",
                    "ignoreFailures": true
                }
            ],
        },
    ]
}